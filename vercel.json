{
  "version": 2,
  "framework": null,
  "functions": {
    "api/*.ts": {
      "memory": 2048,
      "maxDuration": 120
    }
  },
  "routes": [
    { "src": "/docs(.*)", "dest": "/api/index.ts" },
    { "src": "/v1/(.*)", "dest": "/api/index.ts" },
    { "src": "/(.*)", "dest": "/api/index.ts" }
  ]
}
